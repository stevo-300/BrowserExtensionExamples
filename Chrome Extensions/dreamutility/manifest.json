{
  "manifest_version": 2,
  "name": "Dream Utility",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["dream.js"]
    }
  ],
  "background": {
    "scripts": ["dream_back.js"]
  },
  "browser_action":{
    "default_icon": "Assets/images/raw.png",
    "default_popup": "popup/dreamingtext/index.html",
    "default_title": "Dreaming of Tech Utility"
  }
}
